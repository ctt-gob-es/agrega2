        </wsdl:operation>
        <wsdl:operation name="obtenODEsPropuestos">
                
            <wsdl:documentation> 
                Devuelve los odes de todos los usuarios del nodo que estan
                pendientes de publicacion (en estado propuesto)
            </wsdl:documentation>
            <wsdl:input message="impl:obtenODEsPropuestos13Request" 
                        name="obtenODEsPropuestos13Request"/>
            <wsdl:output message="impl:obtenODEsPropuestos13Response" 
