    <wsdl:portType name="SrvBuscadorService">
        <wsdl:operation name="busquedaAvanzada">
                
            <wsdl:documentation> 
                Permite una busqueda avanzada por distintos campos en el
                repositorio de ODEs indexados
                Retorna los documentos que cumplen con la busqueda.
            </wsdl:documentation>
            <wsdl:input message="impl:busquedaAvanzada1Request" 
                        name="busquedaAvanzada1Request"/>
            <wsdl:output message="impl:busquedaAvanzada1Response" 
                         name="busquedaAvanzada1Response"/>
        </wsdl:operation>
        <wsdl:operation name="busquedaAvanzadaNodos">
                
